    "egg-mock": "5.10.7",
    "egg-ts-helper": "^1.34.7",
    "eslint": "8",
    "eslint-config-egg": "12",
    "filemanager-webpack-plugin": "^8.0.0",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "typescript": "5",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4"
  },
